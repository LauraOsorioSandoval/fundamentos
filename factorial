#include <iostream>
using namespace std;
int main()
{
	int n, i, mult=1;
	do {
		cout << "Â¿hasta que numero desea multiplicar?";
		cin >> n;
		if (n < 0)
			cout << "ingrese un numero mayor o igual a 0";
	} while (n <= 0);
	for (i = 1; i <= n; i++)
	{
		mult = mult * i;
	}
	cout << "la multiplicacion es: " << mult;
	return 0;
} 


   
