#include <iostream>
#include <string>

using namespace std;

// Prototipo de la función
string mezclarCadenas( string cadena1, string cadena2);

int main() 
{
    string cadena1, cadena2;
    cout << "Introduce la cadena 1: ";
    cin >> cadena1;
    cout << "Introduce la cadena 2: ";
    cin >> cadena2;

    cout << "Salida: " << mezclarCadenas(cadena1, cadena2) << endl;

    return 0;
}

// Definición de la función
string mezclarCadenas(string cadena1,string cadena2)
{
    return cadena1 + cadena2;
}
